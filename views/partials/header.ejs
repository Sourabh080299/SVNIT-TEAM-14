@keyframes bounce{
  0% {
    top: 600px;
    background-color: #006999;
  }
  70% {
    top: 200px;
    height: 100px;
    background-color: #0d24f1;
  }
  94% {
    height: 90px;
    background-color: #4f0099;
  }
  100% {
    top: 200px;
    height: 90px;
    background-color: #172eb1;
  }
}
.right{
    right:0px;
}
#balls {

  width: 100px;
  height: 100px;
  background-color: #007399;
  border-radius: 50%;
  position: absolute;
  top: 200px;
  -webkit-animation: bounce 1.5s infinite alternate;
  animation: bounce 1.5s infinite alternate;
}


button{
  display: block;
  width: 200px;
  height: 40px;
  line-height: 40px;
  font-size: 18px;
  font-family: sans-serif;
  text-decoration: none;
  color: #333;
  border: 2px solid #333;
  letter-spacing: 2px;
  text-align: center;
  position: relative;
  transition: all .35s;
}

button span{
  position: relative;
  z-index: 2;
}

button:after{
  position: absolute;
  content: "";
  top: 0;
  left: 0;
  width: 0;
  height: 100%;
  background: #1733af;
  transition: all .35s;
}

button:hover{
  color: #fff;
}

button:hover:after{
  width: 100%;
}

